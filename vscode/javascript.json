{
    "scope": "vscode",
    "rules": [
        ["jsExport", "export=(spaceBack() export)"],
        ["jsKeyword", "(<jsExport>)"]
    ],
    "conditions": {
        "state": "language == 'javascript'"
    },
    "commands": [
        [
            "new function",
            "`function() { }`"
        ],
        [
            "js <jsKeyword>",
            "$1"
        ]
    ],
    "functions": []
}