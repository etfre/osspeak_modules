{
    "scope": "vscode",
    "rules": [],
    "conditions": {
        "state": "language == 'python'"
    },
    "commands": [
        [
            "new function",
            "`def ():` {left}_3"
        ],
        ["py import", "import spaceForward()"],
        ["py return", "return spaceForward()"],
        ["py class", "'class :'{left}"],
        ["py yield", "yield spaceForward()"],
        ["py continue", "continue"],
        ["py break", "break"],
        ["py with", "with spaceForward()"],
        ["py true", "True"],
        ["py false", "False"],
        ["py if", "`if :` {left}"], 
        ["py else if", "`elif :` {left}"], 
        ["py else", "`else:` {enter}"], 
        ["for iterable", "`for  in `clipboard.get(): {left}_text.length(clipboard.get()) {left}_5"],
        ["for enumerate", "`for i,  in enumerate(` clipboard.get() `):` {left}_text.length(clipboard.get()) {left}_16"]
    ],
    "functions": []
}