{
    "functions": [
        [
            "repeat(word)",
            "$word"
        ]
    ],
    "rules": [
        [
            "firefox",
            "firefox=vimperator"
        ],
        [
            "code",
            "code='Visual Studio Code'"
        ],
        ["terminal", "terminal='Terminal of Evan'"],
        [
            "windows",
            "(<firefox> | <code> | <terminal>)"
        ]
    ],
    "conditions": {},
    "commands": [
        [
            "open <windows>",
            "window.focus($1)"
        ],
        [
            "close window",
            "window.close()"
        ],
        [
            "start excel",
            "start('C:\\Program Files (x86)\\Microsoft VS Code\\Code.exe')"
        ],
        ["start terminal", "{ctrl+alt+t}"]
    ],
    "scope": ""
}