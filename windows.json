{
    "functions": [
        [
            "repeat(word)",
            "$word"
        ]
    ],
    "rules": [
        [
            "chrome",
            "chrome='Google Chrome'"
        ],
        [
            "code",
            "code='Visual Studio Code'"
        ],
        ["terminal", "terminal='Terminal of Evan'"],
        [
            "windows",
            "(<chrome> | <code> | <terminal>)"
        ]
    ],
    "conditions": {},
    "commands": [
        [
            "open <windows>",
            "window.focus($1)"
        ],
        [
            "close window",
            "window.close()"
        ],
        ["maximize window", "{f11}"],
        [
            "start excel",
            "start('C:\\Program Files (x86)\\Microsoft VS Code\\Code.exe')"
        ],
        ["start terminal", "{ctrl+alt+t}"],
        ["start code", "shell(code)"],
        ["start chrome", "shell('google-chrome &')"]        
    ],
    "scope": ""
}