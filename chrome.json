{
    "scope": "chrome",
    "rules": [],
    "conditions": {
        "title": "Google Chrome"
    },
    "commands": [
        ["new tab", "{ctrl+t}"],
        ["tab left <optional_number>", "{ctrl+pageup}_$1"],
        ["tab right <optional_number>", "{ctrl+pagedown}_$1"],
        ["close tab <optional_number>", "{ctrl+w}_$1"],
        ["go back", "{alt+left}"],
        ["refresh", "{f5}"],
        ["toggle devtools", "{f12}"],
        ["page top", "{home}"],
        ["page bottom", "{end}"],
        ["navigate", "{ctrl+l}"],
        ["south <optional_number>", "{pagedown}_$1"],
        ["north <optional_number>", "{pageup}_$1"],
        ["go to hacker news", "navigateTo(http://news.ycombinator.com)"],
        ["go to google news", "navigateTo(http://news.google.com)"],
        ["go to reddit", "navigateTo(http://reddit.com)"],
        ["go to something awful", "navigateTo(http://forums.somethingawful.com)"],
        ["go to local dev", "navigateTo('http://localhost:8000')"],
        ["silverback gorilla", "7_3"]
    ],
    "functions": [
        ["navigateTo(url)", "{ctrl+l}wait(.2)$url wait(.2){enter}"]
    ]
}